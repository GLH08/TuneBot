# Telegram Bot Token (从 @BotFather 获取)
BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Telegram API (从 https://my.telegram.org 获取，用于解除 50MB 文件限制)
# 如不需要上传大文件，可不配置
TELEGRAM_API_ID=
TELEGRAM_API_HASH=

# 归档频道 ID (以 -100 开头，确保机器人是该频道管理员)
ARCHIVE_CHANNEL_ID=-1001234567890

# 允许使用的用户 ID (自用，仅限自己，多个用逗号分隔)
ALLOWED_USER_IDS=123456789

# 默认下载音质 (128k, 320k, flac, flac24bit)
DEFAULT_QUALITY=320k

# TuneHub API 地址
API_BASE_URL=https://music-dl.sayqz.com

# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
